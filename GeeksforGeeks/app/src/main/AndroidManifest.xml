<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.geeksforgeeks" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/gfg"
        android:label="@string/app_name"
        android:roundIcon="@drawable/gfg"
        android:supportsRtl="true"
        android:theme="@style/Theme.GeeksForGeeks" >
        <activity
            android:name=".HomeActivity"
            android:label="@string/title_activity_home"
            android:theme="@style/Theme.GeeksForGeeks.NoActionBar" >
        </activity>
        <activity
            android:name=".MainActivity"
            android:theme="@style/Theme.Design.NoActionBar" > <!-- Removing action bar from main/splash screen! -->
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>